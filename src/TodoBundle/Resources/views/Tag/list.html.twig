{% extends 'base.html.twig' %}

{% block body %}
    <h1>Tags list</h1>
    <table class="table table-striped table-bordered">
    {% for tag in tags %}
        <tr><td>{{ tag.title }}</td></tr>
    {% else %}
        <tr><td>{% trans %}tag.empty_list{% endtrans %}</td></tr>
    {% endfor %}
    </table>
{% endblock %}